{
	"manifest_version": 3,
	"name": "SyfVerify",
	"version": "1.0",
	"host_permissions": ["*://*/*"],
	"icons": {
		"16": "favicon.ico",
		"32": "favicon.ico",
		"48": "favicon.ico",
		"128": "favicon.ico"
	},

	"permissions": ["activeTab", "scripting"],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content_scripts.js"]
		}
	]
}
